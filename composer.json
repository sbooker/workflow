{
  "name": "sbooker/workflow",
  "description": "Simple workflow for FSM implementation",
  "keywords": ["FSM", "state machine", "workflow"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey Knigin",
      "email": "sergey.knigin.dev@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "php-ds/php-ds": "^1.2"
  },
  "require-dev": {
    "ext-dom": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "phpunit/phpunit": "^10.2"
  },
  "autoload": {
    "psr-4": {
      "Sbooker\\Workflow\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Sbooker\\Workflow\\": "tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
